{
  "name": "ways_smoothness",
  "rules": [
    {
      "name": "noData",
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#999999",
          "width": 0.5,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
      "name": "excellent",
      "filter": [
        "==",
        "smoothness",
        "excellent"
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#1cac2a",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
