{
    "name": "smoothness",
    "rules": [
      {
        "name": "no data",
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#999999",
            "width": 0.5,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "excellent",
        "filter": [
          "==",
          "smoothness",
          "excellent"
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#1cac2a",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "good",
        "filter": [
          "==",
          "smoothness",
          "good"
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#65df35",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "intermediate",
        "filter": [
          "==",
          "smoothness",
          "intermediate"
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#fff301",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "bad",
        "filter": [
          "||",
          [
            "==",
            "smoothness",
            "bad"
          ],
          [
            "==",
            "smoothness",
            "very_bad"
          ]
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#ff9305",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "horrible",
        "filter": [
          "||",
          [
            "==",
            "smoothness",
            "horrible"
          ],
          [
            "==",
            "smoothness",
            "very_horrible"
          ]
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#ff1201",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      },
      {
        "name": "impassable",
        "filter": [
          "==",
          "smoothness",
          "impassable"
        ],
        "symbolizers": [
          {
            "kind": "Line",
            "color": "#ff0363",
            "width": 1,
            "join": "bevel",
            "cap": "square"
          }
        ]
      }
    ]
  }