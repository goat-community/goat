        "smoothness",
        "good"
      ],
      "symbolizers": [
        {
          "kind": "Line",
          "color": "#65df35",
          "width": 1,
          "join": "bevel",
          "cap": "square"
        }
      ],
      "scaleDenominator": {
        "max": 50000
      }
    },
    {
