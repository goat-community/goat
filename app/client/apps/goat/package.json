{
  "name": "@p4b/goat",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "next build",
    "bundle-analyze": "cross-env ANALYZE=true next build",
    "check-dist": "es-check -v",
    "check-size": "size-limit --highlight-less",
    "clean": "rimraf ./.next ./out ./coverage ./tsconfig.tsbuildinfo ./node_modules",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev -p 3000",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "start": "next start",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .ts,.js,.tsx,.jsx --fix",
    "lint:report": "eslint . --format json --output-file ../../lint-results/goat.json",
    "typecheck": "tsc --project ./tsconfig.json --pretty --noEmit"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@hookform/resolvers": "^3.1.1",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@mui/private-theming": "^5.14.5",
    "@p4b/types": "workspace:*",
    "@p4b/ui": "workspace:*",
    "@reduxjs/toolkit": "^1.9.5",
    "@types/mapbox-gl": "^2.7.11",
    "accept-language": "^3.0.18",
    "axios": "^1.4.0",
    "color": "^4.2.3",
    "dayjs": "^1.11.9",
    "eslint": "^8.47.0",
    "geojson": "^0.5.0",
    "i18next": "^23.4.4",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "mapbox-gl": "^2.15.0",
    "maplibre-gl": "^3.1.0",
    "next": "v13.4.12",
    "next-auth": "^4.22.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-drag-drop-files": "^2.3.10",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.1.2",
    "react-map-gl": "^7.1.2",
    "react-redux": "^8.1.1",
    "react-toastify": "^9.1.3",
    "remove-accents": "^0.5.0",
    "screenfull": "^6.0.2",
    "swr": "^2.2.0",
    "tss-react": "latest",
    "uuid": "^9.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.17",
    "@p4b/tsconfig": "workspace:*",
    "@playwright/test": "^1.33.0",
    "@size-limit/file": "^8.2.6",
    "@types/node": "^20.1.5",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.2",
    "cross-env": "^7.0.3",
    "es-check": "^7.1.1",
    "eslint-config-p4b": "workspace:*",
    "size-limit": "^8.2.6"
  }
}
